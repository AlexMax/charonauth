<h2>User Directory</h2>
<table class="table">
	<thead>
		<tr>
			<th>Country</th>
			<th>User</th>
			<th>Clan</th>
			<th>Last Seen</th>
		</tr>
	</thead>
	<tbody>
		{{#users}}
		<tr>
			<td>{{profile.country}}</td>
			<td>{{profile.username}}</td>
			<td>{{profile.clan}}</td>
			<td>{{seen}}
		</tr>
		{{/users}}
	</tbody>
</table>
